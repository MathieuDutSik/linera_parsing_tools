{
    "environments": [
        {
            "command": "./target/debug/linera faucet --amount 1000 --port 8079",
            "environments": [
                "export LINERA_WALLET=/tmp/WORK/wallet_0.json",
                "export LINERA_STORAGE=rocksdb:/tmp/WORK/client_0.db"
            ]
        },
        {
            "command": "cargo test test_wasm_end_to_end_amm::remote_net_grpc --features metrics,remote-net,storage-service",
            "environments": [
                "export LINERA_FAUCET_URL=http://localhost:8079"
            ]
        }
    ],
    "commands": [
        "rm -rf /tmp/WORK",
        "mkdir -p /tmp/WORK",
        "cargo build --features metrics,remote-net,storage-service",
        "./target/debug/linera storage delete_all --storage service:tcp:localhost:1235:table_a",
        "./target/debug/linera net up --policy-config devnet --storage service:tcp:localhost:1235:table_a --path /tmp/WORK &",
        "sleep 20",
        "./target/debug/linera faucet --amount 1000 --port 8079 &",
        "sleep 20"
    ],
    "critical_command": "cargo test test_wasm_end_to_end_amm::remote_net_grpc --features metrics,remote-net,storage-service",
    "target_prometheus_keys_hist": [
        "lru_caching_read_value_bytes",
        "lru_caching_read_multi_value_bytes",
        "lru_caching_contains_key",
        "lru_caching_contains_keys",
        "lru_caching_find_keys_by_prefix",
        "lru_caching_find_key_values_by_prefix",
        "lru_caching_write_batch",
        "lru_caching_clear_journal"
    ],
    "target_prometheus_fault_success": [
        {
            "fault": "num_cache_fault",
            "success": "num_cache_success"
        },
        {
            "fault": "num_cache_find_fault",
            "success": "num_cache_find_success"
        },
        {
            "fault": "num_cache_value_fault",
            "success": "num_cache_value_success"
        }
    ],
    "target_log_keys": [
	"Closing chain confirmed",
        "Transfer confirmed",
        "Application published and created",
	"Application created",
	"Opening a new chain confirmed",
	"Chain linked to key",
	"Key generated",
	"Wallet initialized",
	"Local balance obtained",
	"Synchronized chain information",
	"Balance obtained",
        "Opening a new multi-owner chain confirmed",
        "Data blob published",
        "Data blob read",
	"Processed incoming messages",
	"Bytecode published",
	"Operation confirmed"
    ],
    "target_runtimes": [
        "test_wasm_end_to_end_amm::remote_net_grpc"
    ],
    "l_job_name": [
        "linera_test_11001"
    ],
    "n_iter": 10,
    "skip": 1,
    "kill_after_work": [
        "linera",
        "linera-proxy",
        "linera-server"
    ]
}
